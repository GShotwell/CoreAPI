        <entity><!--Inventory Capacity Mgmt Live Report-->
            <name>Inventory Capacity Mgmt Live Report</name>
            <entityTypeName>LIVE REPORT</entityTypeName>
            <active>1</active>
            <locationName>Core Office</locationName>
            <hasComments>0</hasComments>
