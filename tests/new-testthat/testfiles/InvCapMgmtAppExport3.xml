            <values>
                <value>
                    <attributeName>ci_report_query</attributeName>
                    <data>{&#xd;
    &amp;quot;type&amp;quot;: &amp;quot;sql&amp;quot;,&#xd;
    &amp;quot;query&amp;quot;: &amp;quot;SELECT &amp;apos;&amp;lt;a href=&amp;apos;   ||chr(34)   ||&amp;apos;corelims?cmd=move-here&amp;apos;   ||chr(38)   ||&amp;apos;entityType=&amp;apos;   ||REPLACE(collapsedLocations.entity_type_name, &amp;apos; &amp;apos;, chr(37)   || &amp;apos;20&amp;apos;)   || &amp;apos;&amp;apos;   ||chr(38)   ||&amp;apos;entityId&amp;apos;   ||chr(61)   ||collapsedLocations.entity_id   ||chr(34)   || &amp;apos;&amp;gt;MoveTo_&amp;apos;   ||collapsedLocations.barcode   ||&amp;apos;&amp;lt;/a&amp;gt;&amp;apos; BARCODELINK,   collapsedLocations.barcode,   capacity,   full_location FROM   (SELECT children.barcode,     SUM(grandChildrenLocs.capacity) - SUM(grandChildrenLocs.count) Capacity,     children.full_location,     children.entity_id,     children.entity_type_name   FROM     (SELECT rle.barcode,       rle.entity_id,       rle.entity_type_id,       rle.entity_type_name,       rle.capacity,       rle.count,       rle.full_location     FROM location_entity rle,       (SELECT ENTITY_NAME FROM location_entity WHERE barcode=&amp;apos;${parentBarcode}&amp;apos;       ) fullLocation     WHERE rle.full_location LIKE concat(&amp;apos;%&amp;apos;,concat(fullLocation.entity_name, &amp;apos;%&amp;apos;))     AND active=1     ) children   LEFT JOIN entity_type et   ON et.entity_type_id =children.entity_type_id   LEFT JOIN value   ON value.entity_id=children.entity_id   LEFT JOIN attribute   ON value.attribute_id=attribute.attribute_id   LEFT JOIN location_format   ON location_format.location_format_name=value.string_data   LEFT JOIN entity grandChildren   ON grandChildren.location_id=children.entity_id   LEFT JOIN location_entity grandChildrenLocs   ON grandChildrenLocs.entity_id =grandChildren.entity_id   WHERE et.entity_type_name      =&amp;apos;${locationType}&amp;apos;   AND value.string_data         IS NOT NULL   AND grandChildrenLocs.active   = 1   AND attribute.attribute_name   =&amp;apos;Location Format&amp;apos;   GROUP BY children.barcode,     (children.capacity-children.count),     children.full_location,     children.entity_type_name,     children.entity_id,     value.string_data   HAVING (MIN(grandChildrenLocs.capacity)                                   = -1   OR SUM(grandChildrenLocs.capacity)        - SUM(grandChildrenLocs.count) &amp;gt;= ${minCapacity})   UNION   SELECT barcode,     location_entity.capacity-location_entity.count Capacity,     location_entity.full_location,     location_entity.entity_id,     location_entity.entity_type_name   FROM location_entity   WHERE barcode NOT IN     (SELECT barcode     FROM location_entity     JOIN value     ON value.entity_id=location_entity.entity_id     JOIN attribute     ON attribute.attribute_id             =value.attribute_id     WHERE location_entity.entity_type_name=&amp;apos;${locationType}&amp;apos;     AND attribute.attribute_name          =&amp;apos;Location Format&amp;apos;     AND value.string_data                IS NOT NULL     )   AND location_entity.entity_type_name                  =&amp;apos;${locationType}&amp;apos;   AND location_entity.active                            =1   AND (location_entity.capacity                         - location_entity.count &amp;gt;= ${minCapacity}   OR location_entity.capacity                           = -1)   ) collapsedLocations ORDER BY Capacity,   barcode&amp;quot;,&#xd;
    &amp;quot;model&amp;quot;: {&#xd;
        &amp;quot;fields&amp;quot;: [&#xd;
            {&#xd;
                &amp;quot;fieldKey&amp;quot;: &amp;quot;BARCODE&amp;quot;,&#xd;
                &amp;quot;fieldType&amp;quot;: &amp;quot;BARCODE&amp;quot;,&#xd;
                &amp;quot;fieldLabel&amp;quot;: &amp;quot;Barcode&amp;quot;,&#xd;
                &amp;quot;createGridColumn&amp;quot;: true&#xd;
            },&#xd;
            {&#xd;
                &amp;quot;fieldKey&amp;quot;: &amp;quot;CAPACITY&amp;quot;,&#xd;
                &amp;quot;fieldType&amp;quot;: &amp;quot;NUMERIC_DATA&amp;quot;,&#xd;
                &amp;quot;fieldLabel&amp;quot;: &amp;quot;Capacity&amp;quot;,&#xd;
                &amp;quot;createGridColumn&amp;quot;: true&#xd;
            },&#xd;
            {&#xd;
                &amp;quot;fieldKey&amp;quot;: &amp;quot;BARCODELINK&amp;quot;,&#xd;
                &amp;quot;fieldType&amp;quot;: &amp;quot;STRING_DATA&amp;quot;,&#xd;
                &amp;quot;fieldLabel&amp;quot;: &amp;quot;MoveToLink&amp;quot;,&#xd;
                &amp;quot;createGridColumn&amp;quot;: true&#xd;
            },&#xd;
            {&#xd;
                &amp;quot;fieldKey&amp;quot;: &amp;quot;FULL_LOCATION&amp;quot;,&#xd;
                &amp;quot;fieldType&amp;quot;: &amp;quot;STRING_DATA&amp;quot;,&#xd;
                &amp;quot;fieldLabel&amp;quot;: &amp;quot;Full Location&amp;quot;,&#xd;
                &amp;quot;createGridColumn&amp;quot;: true&#xd;
            },&#xd;
        ]&#xd;
    }&#xd;
}</data>
